{
  "name": "RoyalTS-Document-Api-Server",
  "version": "1.0.0",
  "description": "A Royal TS API for generating documents",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/server.js",
    "postinstall": "npm run build",
    "build": "babel src --out-dir dist --copy-files",
    "build-watch": "babel src --watch --out-dir dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/riemers/royaltsapi.git"
  },
  "keywords": [
    "Royal",
    "Royalts"
  ],
  "author": "Erik-jan Riemers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/riemers/royaltsapi/issues"
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "express": "^4.13.3",
    "royalts": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-export-extensions": "^6.5.0",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-preset-es2015-node-auto": "0.0.5",
    "babel-preset-stage-0": "^6.5.0"
